            <div id="featured-overlay"></div>
        </div>*@ 
<!-- /#featured -->
        @RenderBody()

        <!-- Footer
        ================================================== -->
        <footer id="colophon" class="site-footer">

            <div id="footer-top">
                <div class="container">
                    <div class="widget clearfix">
                        <div class="widget-title-area">
                            <span class="widget-title-inner"></span>
                            <h5 class="widget-title"><span>Follow us on Instagram</span></h5> <!-- /.widget-title -->
                        </div> <!-- /.widget-title-area -->
                        <div id="instafeed"></div> <!-- /.instafeed-content -->
                    </div> <!-- /.glimmer_about_widget -->
                </div>
            </div> <!-- #footer-top" -->

            <div id="footer-middle">
                <div class="container">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="widget widget_text clearfix">
                                <div class="widget-title-area">
                                    <h5 class="widget-title"><span>About</span></h5> <!-- /.widget-title -->
                                </div> <!-- /.widget-title-area -->

                                <div class="widget-contact">
                                    <p>
                                        <img alt="logo" class="retina" src="~/Template/images/glimmer-logo.png">
                                    </p>
                                    <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Cras vitae nibh nisl. Cras et mauris eget lorem ultricies fermentum  diam.</p>
                                </div>
                            </div> <!-- /.widget_about_and_contact -->
                        </div> <!-- /.col-md-4 -->

                        <div class="col-md-4">
                            <div class="widget widget_subscribe clearfix">
                                <div class="widget-title-area">
                                    <h5 class="widget-title"><span>Newsletter</span></h5> <!-- /.widget-title -->
                                </div>
                                <div class="newsletter-area">
                                    <p>Lum eariassi nulliquae con por am et aut minum erepelluptas corepro qui quodi cor em unt.</p>
                                    <div class="form-newsletter">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <p>
                                                    <input type='text' name='name' id='firstname' class="form-controller" aria-required="true" placeholder="Your Name" value="">
                                                </p>
                                            </div>
                                            <div class="col-md-12">
                                                <p>
                                                    <input type='email' name='email' id='email' class="form-controller" aria-required="true" placeholder="Your Email" value="">
                                                </p>
                                            </div>
                                            <div class="col-md-12 text-center">
                                                <button type="submit" id="mc-embedded-subscribe" class="mail-chip-button">Subscribe Us</button>
                                            </div>
                                        </div>
                                    </div> <!-- /.form-newsletter -->
                                </div> <!-- /.newsletter-area -->
                            </div> <!-- /.widget_about_and_contact -->
                        </div> <!-- /.col-md-4 -->

                        <div class="col-md-4">
                            <div class="widget widget_flicker clearfix">
                                <div class="widget-title-area">
                                    <h5 class="widget-title"><span>Flicker Images</span></h5> <!-- /.widget-title -->
                                </div> <!-- /.widget-title-area -->
                            </div> <!-- /.widget_about_and_contact -->
                        </div> <!-- /.col-md-4 -->
                    </div> <!-- /.row -->
                </div> <!-- /.container -->
            </div> <!-- #footer-middle -->

            <div id="footer-bottom">
                <div class="container">
                    <div class="copyright">
                        <p>Copyright&copy; 2015 Softhopper. All right reserved.</p>
                    </div> <!-- /.copyright -->
                </div> <!-- /.container -->
            </div> <!-- /#footer-bottom -->

    </footer><!-- #colophon -->
    

