<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <!-- Basic Page Needs
       ================================================== -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Specific Meta
    ================================================== -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="glimmer is a modern presentation HTML5 Blog template.">
    <meta name="keywords" content="HTML5, Template, Design, Development, Blog" />
    <meta name="author" content="">

    <!-- Titles
    ================================================== -->
    <title>Glimmer | Blog Template</title>

    <!-- Favicons
    ================================================== -->
    <link rel="shortcut icon" href="~/Template/images/favicon.html">
    <link rel="apple-touch-icon" href="~/Template/images/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/Template/images/apple-touch-icon-72x72.html">
    <link rel="apple-touch-icon" sizes="114x114" href="~/Template/images/apple-touch-icon-114x114.html">

    <!-- Custom Font
    ================================================== -->
    <link href='https://fonts.googleapis.com/css?family=Playfair+Display:400,400italic,700,700italic%7CLato:400,400italic,700,700italic' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="~/Template/fonts/font-awsome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Template/fonts/glimmer/glimmer-shape.css">

    <!-- CSS
    ================================================== -->
    <link rel="stylesheet" href="~/Template/lib/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Template/lib/magnific-popup/magnific-popup.css">
    <link rel="stylesheet" href="~/Template/lib/justifiedgallery/justifiedGallery.min.css">
    <link rel="stylesheet" href="~/Template/css/owl.carousel.css">
    <link rel="stylesheet" href="~/Template/style.css">
    <link rel="stylesheet" href="~/Template/colors/color-schemer.css">

    <!-- Modernizr
    ================================================== -->
    <script src="~/Template/js/vendor/modernizr-2.8.3.min.js"></script>

</head>
