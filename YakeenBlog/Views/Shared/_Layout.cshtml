    <script src="~/Template/lib/bootstrap/js/bootstrap.min.js"></script> <!-- bootstrap -->
    <script src="~/Template/js/owl.carousel.min.js"></script> <!-- owl-carousel -->
    <script src="~/Template/lib/magnific-popup/jquery.magnific-popup.min.js"></script> <!-- libs -->
    <script src="~/Template/lib/justifiedgallery/jquery.justifiedGallery.min.js"></script> <!-- justified gallery -->
    @*<script src="~/Template/js/plugins.js"></script> <!-- plugins js -->*@
